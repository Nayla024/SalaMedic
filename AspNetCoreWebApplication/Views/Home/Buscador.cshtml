@using AspNetCoreWebApplication.Models
@model ICollection<alumno>
<h1>Alumnos con Temperatura Alta</h1>

<table class="table table-dark border">
      <tr>
          <th>Id</th>
          <th>Nombre</th>
          <th>Apellido</th>
          <th>Facultad</th>
          <th>Temperatura</th>
      </tr>
      @foreach (var x in Model)
      {
          int a = 0;
          a = int.Parse(x.temp);
          @if (a >= 37)
          {
              <tr>
                <td>@x.id</td>
                <td>@x.nombre</td>
                <td>@x.apellido</td>
                <td>@x.facultad</td>
                <td>@x.temp</td>
            </tr>
          }
      }
  </table>